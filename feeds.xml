---
layout: none
---
<feed xmlns="http://www.w3.org/2005/Atom">
<generator uri="https://jekyllrb.com/" version="3.8.5">Jekyll</generator>
<link href="{{site.url}}/feeds.xml" rel="self" type="application/atom+xml"/>
<link href="{{site.url}}" rel="alternate" type="text/html"/>
//<updated>2020-01-16T05:43:40+00:00</updated>
<id>{{site.url}}/feeds.xml</id>
<title type="html">{{site.title}}</title>
<subtitle>{{site.description}}</subtitle>
{% for post in site.posts %}
    {% if post.project == null %}
<entry>
<title type="html">{{ post.title | xml_escape }}</title>
<link href="{{ site.url }}/{{ post.url }}" rel="alternate" type="text/html" title="{{ post.title | xml_escape }}"/>
<published>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</published>
<updated>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</updated>
<id>{{ site.url }}/{{ post.url }}</id>
<content type="html" xml:base="{{ site.url }}/{{ post.url }}">
{{post.content}}
</content>
<author>
<name/>
</author>
<summary type="html">
{{post.excerpt | xml_escape}}
</summary>
</entry>
<% endif %>
{% endfor %}
</feed>
